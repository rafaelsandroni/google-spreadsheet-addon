{
  "root": true,
  "extends": "eslint-config-es5",
  "parserOptions": {"ecmaVersion": 5},
  "plugins": ["googleappsscript"],
  "env": { "googleappsscript/googleappsscript": true },
  "rules": {
    "semi": ["error", "never"],
    "space-before-function-paren": ["error", "always"],
    "no-multi-str": "off",
    "no-unused-vars": "off",
    "func-names": ["warn", "never"],
    "no-eq-null": "off",
    "eqeqeq": ["error", "smart"]
  },
  "globals": {
    "ApiClient_": "readonly",
    "Connection_": "readonly",
    "formatDatetimeField_": "readonly",
    "formatNumber_": "readonly",
    "NoDataError_": "readonly",
    "HistoricDataForbiddenError_": "readonly",
    "handleErrors_": "readonly",
    "assertCanAccessHistoricData_": "readonly",
    "assertHasData_": "readonly",
    "setUserProperty_": "readonly",
    "apiKeyProperty_": "readonly",
    "hasApiKeyProperty_": "readonly",
    "API_KEY": "readonly",
    "toUTC_": "readonly",
    "dailyPrices_": "readonly",
    "allProjects_": "readonly",
    "getApiClient_": "readonly",
    "erc20Projects_": "readonly",
    "dailyTransactionVolume_": "readonly",
    "dailyOhlc_": "readonly",
    "dailyPriceVolumeDiff_": "readonly",
    "socialVolumeProjects_": "readonly",
    "dailySocialVolume_": "readonly",
    "dailyGithubActivity_": "readonly",
    "dailyDevActivity_": "readonly",
    "dailyNetworkGrowth_": "readonly",
    "dailyExchangeFundsFlow_": "readonly",
    "dailyTokenCirculation_": "readonly",
    "dailyTrendingWords_": "readonly",
    "fetchProjectFundamentals_": "readonly",
    "projectSocialData_": "readonly",
    "mvrvRatio_": "readonly",
    "nvtRatio_": "readonly",
    "dailyActiveDeposits_": "readonly",
    "realizedValue_": "readonly",
    "gasUsed_": "readonly"
  }
}
