{
  "root": true,
  "extends": "eslint-config-es5",
  "parserOptions": {"ecmaVersion": 5},
  "plugins": ["googleappsscript"],
  "env": { "googleappsscript/googleappsscript": true },
  "rules": {
    "semi": ["error", "never"],
    "space-before-function-paren": ["error", "always"],
    "no-multi-str": "off",
    "no-unused-vars": "off",
    "func-names": ["warn", "never"],
    "no-eq-null": "off",
    "eqeqeq": ["error", "smart"]
  },
  "globals": {
    "ApiClient_": "readonly",
    "Connection_": "readonly",
    "formatDatetimeField_": "readonly",
    "formatNumber_": "readonly",
    "NoDataError_": "readonly",
    "HistoricDataForbiddenError_": "readonly",
    "handleErrors_": "readonly",
    "checkForHistoricData_": "readonly",
    "requestedDataIsHistoric_": "readonly",
    "assertCanAccessHistoricData_": "readonly",
    "assertHasData_": "readonly",
    "setUserProperty_": "readonly",
    "apiKeyProperty_": "readonly",
    "hasApiKeyProperty_": "readonly",
    "API_KEY": "readonly",
    "toUTC_": "readonly"
  }
}
